{
  "manifest_version": 2,
  "key": "",
  "name": "Interface NFePHP A3",
  "version": "1.0",
  "description": "Interface de comunicação com A3",
  "background": {
      "scripts": [ "background.js" ]
    },
    "content_scripts": [
    {
      "matches": [ "http://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "permissions": [
    "nativeMessaging"
  ]
}
